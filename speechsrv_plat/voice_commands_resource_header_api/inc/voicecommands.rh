//  STRUCTURE DEFINITIONS

// -----------------------------------------------------------------------------
// VCOMMANDITEM
// One Voice Command text item
// -----------------------------------------------------------------------------
//
STRUCT VCOMMANDITEM
    {
    WORD key;
    LTEXT txt; // variable length
    }

// -----------------------------------------------------------------------------
// VCOMMANDS
// Array of Voice Command text items
// -----------------------------------------------------------------------------
//
STRUCT VCOMMANDS
    {
    STRUCT items[];
    }

// -----------------------------------------------------------------------------
// VCOMMANDITEM
// One Voice Command integer item
// -----------------------------------------------------------------------------
//
STRUCT VCOMMANDINTEGERITEM
    {
    WORD key;
    WORD value;
    }

// -----------------------------------------------------------------------------
// VCOMMANDS
// Array of Voice Command text items
// -----------------------------------------------------------------------------
//
STRUCT VCOMMANDINTEGERS
    {
    STRUCT intitems[];
    }


